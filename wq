[32m+[m[41m		[m
[32m+[m		[32m<script>[m
[32m+[m		[32mlet seats = document.querySelectorAll('.seat');[m
[32m+[m[41m			[m
[32m+[m		[32mlet selectedSeats = []; // 선택된 좌석 배열. 배열에 넣어준다.[m
[32m+[m
[32m+[m		[32mseats.forEach(function(seat) {[m
[32m+[m			[32mseat.addEventListener('click', function() {[m
[32m+[m				[32mif (this.classList.contains('selected')) {[m
[32m+[m					[32m// 이미 선택된 좌석인 경우, 선택 취소[m
[32m+[m					[32mthis.classList.remove('selected');[m
[32m+[m					[32m//선택한 좌석의 인덱스에서 요소 1개만 삭제 = 본인만 삭제[m
[32m+[m					[32mselectedSeats.splice(selectedSeats.indexOf(this), 1);[m
[32m+[m				[32m} else if (selectedSeats.length < 2) {[m
[32m+[m					[32m// 선택되지 않은 좌석이고 최대 선택 가능한 개수보다 작을 경우, 선택[m
[32m+[m					[32mthis.classList.add('selected');[m
[32m+[m					[32mselectedSeats.push(this);[m
[32m+[m				[32m} else{[m
[32m+[m					[32m// 좌석 2개 초과 선택 시, 초과 선택불가 알럿[m
[32m+[m					[32malert('좌석은 최대 2개 선택 가능합니다.')[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m});[m
[32m+[m		[32m});[m
[32m+[m		[32m</script>[m
[32m+[m[41m		[m
[32m+[m		[32m<div>선택한 좌석 수: <span>0</span></div>[m
[32m+[m		[32m<div>총 금액: <span>0</span></div>[m
[32m+[m[41m		[m
[32m+[m		[32m<div><button class="nextButton">다음단계</button></div>[m
[32m+[m	[32m</body>[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[32m</body>[m
