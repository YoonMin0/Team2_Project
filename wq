[32m+[m							[32m<h3>제목</h3>[m
[32m+[m[32m<!--							300자 제한-->[m
[32m+[m							[32m<p class="ReviewContent">[m
[32m+[m							[32m국가안전보장회의의 조직·직무범위 기타 필요한 사항은 법률로 정한다. 공무원은 국민전체에 대한 봉사자이며, 국민에 대하여 책임을 진다. 정부는 예산에 변경을 가할 필요가 있을 때에는 추가경정예산안을 편성하여 국회에 제출할 수 있다.[m
[32m+[m							[32m대한민국의 영토는 한반도와 그 부속도서로 한다. 국회는 국민의 보통·평등·직접·비밀선거에 의하여 선출된 국회의원으로 구성한다. 탄핵소추의 의결을 받은 자는 탄핵심판이 있을 때까지 그 권한행사가 정지된다.</p>[m
[32m+[m							[32m<button class="ReviewMore" onclick=button_onclick()>후기 더보기</button>[m
[32m+[m
[32m+[m						[32m</div>[m
[32m+[m					[32m</div>[m
[32m+[m[41m					[m
[32m+[m		[32m</div>[m
[32m+[m[41m		[m
[32m+[m		[32m<script>[m
[32m+[m[41m			[m
[32m+[m		[32m//후기 더보기 , 접기 버튼[m
[32m+[m[41m		[m
[32m+[m		[32m$('.ReviewMore').on('click', function() {[m
[32m+[m			[32mif ($('.ReviewMore').html() == '후기 더보기') {[m
[32m+[m				[32m// 후기 더보기 버튼 클릭 시[m
[32m+[m				[32m$('.ReviewContent').addClass('open'); // 열리는 클래스 추가[m
[32m+[m				[32m$('.ReviewMore').html('접기');[m
[32m+[m			[32m} else {[m
[32m+[m				[32m// 접기 버튼 클릭 시[m
[32m+[m				[32m$('.ReviewContent').removeClass('open'); // 열리는 클래스 제거[m
[32m+[m				[32m$('.ReviewMore').html('후기 더보기');[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m[41m			[m
[32m+[m		[32m</script>[m
[32m+[m[41m		[m
[32m+[m		[32m<script>[m
[32m+[m
[32m+[m
[32m+[m[41m			[m
[32m+[m		[32m//하단 공연정보/판매정보/관람후기 탭 바꾸기 스크립트[m
[32m+[m[41m			[m
[32m+[m			[32mconst tabs = ['saleInfo', 'playInfo', 'playReview'];[m
[32m+[m			[32mconst details = ['saleInfoDetail', 'playInfoDetail', 'playReviewDetail'];[m
[32m+[m
[32m+[m			[32mtabs.forEach((tab, index) => {[m
[32m+[m				[32mdocument.getElementsByClassName(tab)[0].addEventListener('click', function() {[m
[32m+[m					[32m// 모든 탭의 스타일 및 관련 정보를 초기화[m
[32m+[m					[32mtabs.forEach((otherTab, otherIndex) => {[m
[32m+[m						[32mdocument.getElementsByClassName(otherTab)[0].style.borderBottom = 'none';[m
[32m+[m						[32mdocument.getElementsByClassName(details[otherIndex])[0].style.display = 'none';[m
[32m+[m					[32m});[m
[32m+[m
[32m+[m				[32m//초기화 후 클릭된 탭과 관련된 정보 표시[m
[32m+[m				[32mdocument.getElementsByClassName(tab)[0].style.borderBottom = '5px solid';[m
[32m+[m				[32mdocument.getElementsByClassName(details[index])[0].style.display = 'block';[m
[32m+[m				[32m});[m
[32m+[m[41m				[m
[32m+[m			[32m});[m
[32m+[m[41m			[m
[32m+[m		[32m</script>[m
[32m+[m	[32m</body>[m
[32m+[m[41m	[m
[32m+[m	[32m<footer class="footer"></footer>[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
