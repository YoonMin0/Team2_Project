[1m+++ b/src/main/webapp/WEB-INF/views/inquiry/inquiry.js[m
[36m@@ -0,0 +1,157 @@[m
[32m+[m[32mconst maxLength = 300;[m
[32m+[m[32mlet selectedFiles = []; // 선택된 파일을 저장할 배열[m
[32m+[m[32mlet isDropDown = true;[m
[32m+[m
[32m+[m[32mfunction inquiryContentHandler() {[m
[32m+[m[32m  let inquiryContent = document.querySelector('#inquiry-content');[m
[32m+[m[32m  let errorMessageElement = document.querySelector('.error-message');[m
[32m+[m[32m  let charCountElement = document.querySelector('.char-count');[m
[32m+[m[32m  errorMessageElement.style.color = 'red';[m
[32m+[m[32m  charCountElement.style.color = 'black';[m
[32m+[m[32m  let contentLength = inquiryContent.value.length;[m
[32m+[m
[32m+[m[32m  // 글자수 업데이트[m
[32m+[m[32m  charCountElement.textContent = contentLength;[m
[32m+[m
[32m+[m[32m  if (contentLength < 10) {[m
[32m+[m[32m      errorMessageElement.innerHTML = '<div class="error-icon"></div>' + '<div style="display:inline-block;">10자 이상의 문의 내용을 입력해 주세요.</div>';[m
[32m+[m[32m      errorMessageElement.style.visibility = 'visible'; // Show message[m
[32m+[m[32m  } else {[m
[32m+[m[32m      errorMessageElement.style.visibility = 'hidden'; // Hide message[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (contentLength >= 10 && contentLength < maxLength) {[m
[32m+[m[32m      errorMessageElement.style.visibility = 'hidden'; // Hide message[m
[32m+[m[32m  } else if (contentLength == maxLength) {[m
[32m+[m[32m      charCountElement.style.color = 'red';[m
[32m+[m[32m  } else if (contentLength > maxLength) {[m
[32m+[m[32m      charCountElement.style.color = 'red';[m
[32m+[m[32m      setupTextAreaLimitWithModal();[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction setupTextAreaLimitWithModal() {[m
[32m+[m[32mconst textarea = document.getElementById('inquiry-content');[m
[32m+[m[32mconst charCount = document.querySelector('.char-count');[m
[32m+[m[32mconst modal = document.getElementById('maxLengthModal');[m
[32m+[m[32mconst closeModal = document.getElementById('closeModal');[m
[32m+[m
[32m+[m
[32m+[m[32mfunction updateCharCount() {[m
[32m+[m[32m    const currentLength = textarea.value.length;[m
[32m+[m[32m    charCount.textContent = currentLength;[m
[32m+[m
[32m+[m[32m    if (currentLength > maxLength) {[m
[32m+[m[32m        modal.style.display = 'block';[m
[32m+[m[32m        textarea.value = textarea.value.slice(0, maxLength); // 300자로 잘라냄[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mtextarea.addEventListener('input', updateCharCount);[m
[32m+[m
[32m+[m[32mcloseModal.addEventListener('click', function() {[m
[32m+[m[32m    modal.style.display = 'none';[m
[32m+[m[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mfunction inquiryCategoryHandler() {[m
[32m+[m[32m  let arrowDown = document.querySelector('.select-arrow');[m
[32m+[m[32m  let arrowUp = document.querySelector('.select-arrow2');[m
[32m+[m
[32m+[m[32m  if(isDropDown) {[m
[32m+[m[32m    arrowDown.style.visibility = 'hidden';[m
[32m+[m[32m    arrowUp.style.visibility = 'visible';[m
[32m+[m[32m    isDropDown = false;[m
[32m+[m[32m  }[m
[32m+[m[32m  else {[m
[32m+[m[32m    arrowDown.style.visibility = 'visible';[m
[32m+[m[32m    arrowUp.style.visibility = 'hidden';[m
[32m+[m[32m    isDropDown = true;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mfunction init() {[m
[32m+[m[32m  document.querySelector('#inquiry-content').addEventListener('input', inquiryContentHandler);[m
[32m+[m
[32m+[m[32m  document.querySelector('.form-select').addEventListener('click', inquiryCategoryHandler);[m
[32m+[m
[32m+[m[32m  document.getElementById('file-input').addEventListener('change', function(event) {[m
[32m+[m[32m      var imagePreview = document.getElementById('image-preview');[m
[32m+[m[41m      [m
[32m+[m[32m      // 선택된 파일들을 배열로 변환[m
[32m+[m[32m      var files = Array.from(event.target.files);[m
[32m+[m
[32m+[m[32m      // 현재 선택된 파일의 총 개수[m
[32m+[m[32m      const totalFiles = selectedFiles.length + files.length;[m
[32m+[m
[32m+[m[32m      // 최대 3개까지만 유지[m
[32m+[m[32m      if (totalFiles > 3) {[m
[32m+[m[32m          alert("최대 3개까지만 첨부할 수 있습니다.");[m
[32m+[m[32m          return; // 새로운 파일을 추가하지 않음[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // 선택된 파일들을 기존 배열에 추가[m
[32m+[m[32m      selectedFiles = selectedFiles.concat(files);[m
[32m+[m
[32m+[m[32m      // 미리보기 업데이트[m
[32m+[m[32m      updateImagePreview(imagePreview);[m
[32m+[m[41m      [m
[32m+[m[32m      // 파일 개수 업데이트[m
[32m+[m[32m      document.getElementById('file-count').textContent = selectedFiles.length;[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // 미리보기 업데이트 함수[m
[32m+[m[32m  function updateImagePreview(imagePreview) {[m
[32m+[m[32m      imagePreview.innerHTML = ''; // 기존 미리보기 삭제[m
[32m+[m
[32m+[m[32m      selectedFiles.forEach(function(file, index) {[m
[32m+[m[32m          var reader = new FileReader();[m
[32m+[m
[32m+[m[32m          reader.onload = function(e) {[m
[32m+[m[32m              var imgContainer = document.createElement('div');[m
[32m+[m[32m              imgContainer.style.position = 'relative';[m
[32m+[m[32m              imgContainer.style.display = 'inline-block';[m
[32m+[m[32m              imgContainer.style.margin = '5px';[m
[32m+[m
[32m+[m[32m              var img = document.createElement('img');[m
[32m+[m[32m              img.src = e.target.result;[m
[32m+[m[32m              img.style.width = '120px'; // 미리보기 크기 조정[m
[32m+[m[32m              img.style.height = '120px';[m
[32m+[m
[32m+[m[32m              var deleteButton = document.createElement('button');[m
[32m+[m[32m              deleteButton.textContent = 'X';[m
[32m+[m[32m              deleteButton.style.position = 'absolute';[m
[32m+[m[32m              deleteButton.style.top = '0';[m
[32m+[m[32m              deleteButton.style.right = '0';[m
[32m+[m[32m              deleteButton.style.backgroundColor = 'black';[m
[32m+[m[32m              deleteButton.style.background = 'rgba(0, 0, 0, 0.4)';[m
[32m+[m[32m              deleteButton.style.borderRadius = '6px';[m
[32m+[m[32m              deleteButton.style.color = 'white';[m
[32m+[m[32m              deleteButton.style.border = 'none';[m
[32m+[m[32m              deleteButton.style.cursor = 'pointer';[m
[32m+[m
[32m+[m[32m              // 삭제 버튼 클릭 시 동작[m
[32m+[m[32m              deleteButton.addEventListener('click', function() {[m
[32m+[m[32m                  selectedFiles.splice(index, 1); // 선택된 파일에서 해당 파일 제거[m
[32m+[m[32m                  updateImagePreview(imagePreview); // 미리보기 업데이트[m
[32m+[m[32m                  document.getElementById('file-count').textContent = selectedFiles.length; // 파일 개수 업데이트[m
[32m+[m[32m              });[m
[32m+[m
[32m+[m[32m              imgContainer.appendChild(img);[m
[32m+[m[32m              imgContainer.appendChild(deleteButton);[m
[32m+[m[32m              imagePreview.appendChild(imgContainer);[m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          reader.readAsDataURL(file); // 파일을 읽어 Data URL로 변환[m
[32m+[m[32m      });[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mwindow.addEventListener('load', init);[m
\ No newline at end of file[m
